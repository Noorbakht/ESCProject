 => {\n                    console.log(\"[DEMO] :: Rainbow SDK is initialized!\");\n                })\n                .catch(err => {\n                    console.log(\n                        \"[DEMO] :: Something went wrong with the SDK...\",\n                        err\n                    );\n                });\n            console.log(\"onLoaded()\");\n        },\n        getLiveData() {\n            axios\n                .get(\n                    \"https://limitless-sierra-11102.herokuapp.com/availability\"\n                )\n                .then(response => {\n                    console.log(response.data);\n                    console.log(response.data.totalBank);\n                    console.log(response.data.totalInvest);\n                    console.log(response.data.totalGE);\n                    console.log(response.data.availBank);\n                    console.log(response.data.availInvest);\n                    console.log(response.data.availGE);\n                    this.investment = response.data.availInvest;\n                    this.banking = response.data.availBank;\n                    this.GE = response.data.availGE;\n                });\n        }\n    },\n    beforeDestroy() {\n        clearInterval(this.timer);\n    }\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n/* h3 {\n    margin: 40px 0 0;\n}\nul {\n    list-style-type: none;\n    padding: 0;\n}\nli {\n    display: inline-block;\n    margin: 0 10px;\n    text-decoration: none;\n}\na {\n    color: #42b983;\n} */\n.header {\n    background-image: url(\"../assets/bank.jpg\");\n    color: #fff;\n    text-align: center;\n    padding: 300px;\n    overflow: hidden;\n}\n.header a {\n    color: #fff;\n    padding-right: 5px;\n    text-decoration: none;\n}\nhtml,\nbody {\n    max-height: 100%;\n    padding-bottom: 0 !important;\n    margin: 0;\n    overflow-y: hidden;\n    position: fixed;\n}\n#card {\n    margin-top: 140px;\n    z
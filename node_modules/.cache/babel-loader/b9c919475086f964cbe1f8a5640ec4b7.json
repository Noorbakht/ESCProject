nexport default {\r\n  name: \"Chatbot\",\r\n  components: {},\r\n  data: () => ({\r\n    message: \"\",\r\n    messages: [],\r\n    items: [\r\n      {\r\n        message:\r\n          \"Hi there! You've been connected with our agent. You may start typing to chat!\",\r\n        sender: \"Agent One\",\r\n        time: moment().format(\"h:mm a\")\r\n      }\r\n    ]\r\n  }),\r\n  created() {\r\n    document.addEventListener(rainbowSDK.RAINBOW_ONLOADED, this.onLoaded);\r\n    rainbowSDK.start();\r\n    rainbowSDK.load();\r\n  },\r\n  methods: {\r\n    gettingConnection: function() {\r\n      console.log(\"here in chatbot page\");\r\n      //get agent token id\r\n      //sign in to rainbow using token id\r\n      var applicationID = \"\";\r\n      var applicationSecret = \"\";\r\n      rainbowSDK.setVerboseLog(false);\r\n      rainbowSDK.connection.signin(applicationID, applicationSecret);\r\n      console.log(\"signed in\");\r\n    },\r\n    sendMessage() {\r\n      const message = this.message;\r\n      this.messages.push({\r\n        text: message,\r\n        author: \"client\",\r\n        time: moment().format(\"h:mm a\")\r\n      });\r\n      this.message = \"\";\r\n      //below is replying to user\r\n      // this.$axios\r\n      //   .get(\r\n      //     `https://www.cleverbot.com/getreply?key=CC8uqcCcSO3VsRFvp5-uW5Nxvow&input=${message}`\r\n      //   )\r\n      //   .then(res => {\r\n      //     this.messages.push({\r\n      //       text: res.data.output,\r\n      //       author: \"server\"\r\n      //     });\r\n      //     this.$nextTick(() => {\r\n      //       this.$refs.chatbox.scrollTop = this.$refs.chatbox.scrollHeight;\r\n      //     });\r\n      //   });\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style scoped lang=\"scss\">\r\n.chat-box-list {\r\n  display: flex;\r\n  flex-direction: column;\r\n  list-style-type: none;\r\n}\r\n.chat-box-list-container {\r\n  // overflow: scroll;\r\n  margin-bottom: 1px;\r\n}\r\n.chat-box-list {\r\n  padding-left: 